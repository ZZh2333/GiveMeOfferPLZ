    "def wanmei(n,k):\n",
    "    if n==1:\n",
    "        return k\n",
    "    if(n % 2 == 0):\n",
    "        return wanmei(n/2,k+1)\n",
    "    else:\n",
    "        return min(wanmei(n*2+2,k),wanmei(n*2-2,k))\n",
    "print(wanmei(4,0))"
   ]
